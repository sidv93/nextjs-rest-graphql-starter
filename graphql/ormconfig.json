{
    "name": "default",
    "type": "mongodb",
    "host": "mongo",
    "port": 27017,
    "username": "username",
    "password": "password",
    "database": "dbname",
    "synchronize": true,
    "logging": false,
    "entities": [
        "build/entities/**/*.js"
    ],
    "migrations": [
        "src/migration/**/*.ts"
    ],
    "subscribers": [
        "src/subscriber/**/*.ts"
    ],
    "cli": {
        "entitiesDir": "src/entities",
        "migrationsDir": "src/migration",
        "subscribersDir": "src/subscriber"
    }
}