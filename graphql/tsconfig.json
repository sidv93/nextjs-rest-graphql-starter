{
   "compilerOptions": {
      "lib": [
         "es5",
         "es6",
         "dom",
         "es2018",
         "esnext.asynciterable"
      ],
      "target": "es2018",
      "module": "commonjs",
      "moduleResolution": "node",
      "outDir": "./build",
